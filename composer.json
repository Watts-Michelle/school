{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.3.1",
		"silverstripe/framework": "3.3.1",
		"silverstripe/reports": "3.3.1",
		"silverstripe/siteconfig": "3.3.1",
		"silverstripe-themes/simple": "3.1.*",
		"heyday/silverstripe-cacheclear": "^0.2.1",
		"silverstripe/tagfield": "^1.2",
		"praxisnetau/silverstripe-moderno-admin": "@dev",
		"ajshort/silverstripe-gridfieldextensions": "@dev",
		"undefinedoffset/sortablegridfield": "@dev",
		"jonom/focuspoint": "dev-master"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.3.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
